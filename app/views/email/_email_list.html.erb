  <div class="row">
    <div class="col-6">
    <% if email.read %>
        <p><%= link_to "#{email.object}", email_path(email.id), remote: true%></p>
      <% else %>
        <strong><p><%= link_to "#{email.object}", email_path(email.id), remote: true%></p></strong>
      <%end%> 
    </div>
  </div>